{
  "expo": {
    "name": "TicTacToeApp",
    "description": "Simple Tic-Tac-Toe App with multi player support",
    "slug": "TicTacToeApp",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "githubUrl": "https://github.com/vtapadia/TicTacToeApp",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
